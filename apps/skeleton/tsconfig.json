{
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"@components/*": ["src/components/*"],
			"@lib/*": ["src/lib/*"],
			"@styles/*": ["src/styles/*"],
			"@public/*": ["public/*"],
			"@data/*": ["src/data/*"]
		},
		"incremental": true
	},
	"rootDir": "src",
	"extends": "../../packages/tsconfig/nextjs.json",
	"include": [
		"types.d.ts", "**/*.ts", "**/*.tsx"],
	"exclude": ["node_modules"]
}
