{
	"name": "nextjs-skeleton",
	"version": "1.0.0",
	"private": true,
	"license": "MIT",
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"lint": "turbo run lint"
	},
	"devDependencies": {
		"turbo": "^1.0.12"
	},
	"volta": {
		"node": "14.18.2",
		"npm": "8.3.0"
	},
	"turbo": {
		"baseBranch": "origin/main",
		"pipeline": {
			"build": {
				"dependsOn": [
					"^build"
				],
				"outputs": [
					".next/**"
				]
			},
			"test": {
				"dependsOn": [
					"^build"
				],
				"outputs": []
			},
			"lint": {
				"outputs": []
			},
			"dev": {
				"cache": false
			}
		}
	}
}
